.section4
  height 340px
  padding 52px 0 0 0
  text-align center
  background url("../images/4_img.png")
  color #ffffff
  .title
    font-size 21px
  .line
    width 31px
    border-bottom 3px solid #ffffff
    margin 12px auto
  .content
    font-size 12px
  .city-select
    padding-top 51px
    label
      width 236px
      height 36px
      display inline-block
      background #ffffff
      margin-right 30px
      select
        width 236px
        height 36px
        padding-left 12px
        font-size 12px
        color #a8aaaa
        appearance none
        -webkit-appearance: none
        -moz-appearance: none
        border none
        background  url("../images/4_down.png") no-repeat 210px center
        &:focus
          border 1px solid #e74f4d
          outline 1px solid #e74f4d
        option
          border 1px solid #e74f4d
          outline 1px solid #e74f4d
          height 27px
          &:hover
            background-color #e25c60
    button
      width 236px
      height 36px
      background #e25c60
      border none
      outline none
      font-size 14px
      color #ffffff
      cursor pointer
