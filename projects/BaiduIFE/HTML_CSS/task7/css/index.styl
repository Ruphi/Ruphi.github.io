*
  margin: 0
  padding: 0
  box-sizing border-box
body
  min-width 1280px
  background gainsboro
.f-left
  float: left
.f-right
  float right
.clear
  clear both
ul
  list-style none
.msg-font-yahei
  font-family "Microsoft YaHei UI"
.msg-font-song
  font-family "宋体"
#wrap
  max-width: 1280px
  margin: 0 auto
  background white
header
  height 57px
  padding 14px 0 0 0
  .brand
    ul
      height 36px
      margin-left 59px
      li
        display inline-block
        img
          display block
          margin-top 2px
        font-size 21px
  nav
    ul
      li
        display inline-block
        margin-right 38px
        width 64.5px
        font-size 14px;
        text-align center
        height 41px
        color gray
        &:hover
          cursor pointer
          border-bottom 2px solid #e25c60
          color #e25c60
      li.active
        border-bottom 2px solid #e25c60
        color #e25c60
      li.login
        color #e25c60
.section1
  background url("../images/1_img.png") no-repeat
  height 853px
  padding 227px 0 0 112px
  span
    color #ffffff
    display block
  span.tit
    font-size 60px
    padding-bottom 29px
  span.msg
    font-size 21px
  .button
    width 331px
    height 65px
    background #e74f4d
    font-size 30px
    color white
    cursor pointer
    text-align center
    line-height 65px
    margin-top 62px
